# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
GET     /login                      controllers.LoginController.login
GET     /signup                     controllers.LoginController.signup
GET     /logout                     controllers.LoginController.logout
GET     /artists                    controllers.ArtistsController.index
GET     /artists/create             controllers.ArtistsController.create
GET     /recommender/loadData       controllers.RecommendationsController.loadData
GET     /recommend     controllers.RecommendationsController.recommend(numberNeighbors:Integer)



GET     /test                       controllers.HomeController.test

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
